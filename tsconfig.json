{
		"display": "Node 20",
		"_version": "20.1.0",
		"compilerOptions": {
		"baseUrl": "./src",
		
		"moduleResolution": "node16",
		"allowSyntheticDefaultImports": true,
		"noEmit": false,
		"noEmitOnError": false,
		"alwaysStrict": true,
		"esModuleInterop": true,
		"module": "Node16",
		"noImplicitAny": true,
		"target": "ES2022",
		"removeComments": true,
		"preserveConstEnums": true,
		"outDir": "./dist",
		"rootDir": "./src/",
		"sourceMap": true,
		"lib": ["ES2022"],
		"resolveJsonModule": true,
		"experimentalDecorators": true,
		"emitDecoratorMetadata": true
	}
}